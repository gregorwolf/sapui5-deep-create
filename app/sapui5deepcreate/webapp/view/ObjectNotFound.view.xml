<mvc:View controllerName="udina.sample.sapui5deepcreate.controller.NotFound" 
    xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc">
    <VBox renderType="Bare" justifyContent="Center" fitContainer="true">
        <IllustratedMessage illustrationType="sapIllus-NoData"
            title="{i18n>ObjectNotFound.text}" description="{i18n>ObjectNotFound.description}">
            <additionalContent>
                <Button text="{i18n>ObjectNotFound.link}" type="Emphasized" press=".onNavBack"/>
            </additionalContent>
        </IllustratedMessage>
    </VBox>
</mvc:View>